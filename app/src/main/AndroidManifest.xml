<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.edu.utar.e_votingapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EVotingApp">
        <activity android:name=".SelfieUpload"></activity>
        <activity android:name=".AdminVoteStatus" />
        <activity android:name=".VoterStatus" />
        <activity android:name=".ICImageUpload" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserInfo" />
        <activity android:name=".AdminVerify" />
        <activity android:name=".AdminReport" />
        <activity android:name=".LoginToken" />
        <activity android:name=".Token" />
        <activity android:name=".AdminEdit" />
        <activity android:name=".AdminResult" />
        <activity android:name=".AdminActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".PhoneAuth" />
        <activity android:name=".Login" />
        <activity android:name=".Register" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>